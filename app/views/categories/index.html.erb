<h1>Categories - all categories</h1>


<% @category.each do |category| %>
<% if current_user.admin? %>
<%= link_to "Edit",edit_category_path(category) %>
<%= link_to "Delete", category,method: :delete, data: {confirm: "You Sure"},title: category.category_name %>
<% end %>
<%= category.category_name %>
<br/>
<br/>


<% end %>
